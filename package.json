{
	"name": "etGames",
	"private": true,
	"version": "Spring '25 (v63)",
	"description": "Learn AgentForce the fun way",
	"scripts": {
		"build": "./build.sh",
		"initializeBase": "node ~/ET_Tools/@ELTOROit/ScratchOrgBuilder/orgBuilder.mjs --config-file=@ELTOROIT/scripts/orgBuilder/SandboxBuilderBase.jsonc",
		"openDEV04": "sf org open --target-org aperez.montreal@test1.org.dev04",
		"exportData": "~/ET_Tools/node_modules/sfdx-cli/bin/run ETCopyData export --configfolder ./@ELTOROIT/data --loglevel trace --json",
		"importData": "~/ET_Tools/node_modules/sfdx-cli/bin/run ETCopyData import --configfolder ./@ELTOROIT/data --loglevel trace --json",
		"deleteData": "~/ET_Tools/node_modules/sfdx-cli/bin/run ETCopyData delete --configfolder ./@ELTOROIT/data --loglevel trace --json",
		"createScratchOrg": "node ~/ET_Tools/@ELTOROit/ScratchOrgBuilder/orgBuilder.mjs --config-file=@ELTOROIT/scripts/orgBuilder/ScratchOrgBuilder.jsonc"
	},
	"author": "Andres Perez (@ELTOROIT)",
	"license": "ISC",
	"devDependencies": {
		"@prettier/plugin-xml": "2.2.0",
		"@salesforce/eslint-config-lwc": "3.4.0",
		"@salesforce/eslint-plugin-aura": "2.1.0",
		"@salesforce/sfdx-lwc-jest": "1.3.0",
		"eslint": "8.40.0",
		"eslint-config-prettier": "8.8.0",
		"lint-staged": "13.2.2",
		"prettier": "3.4.2",
		"prettier-plugin-apex": "2.2.2"
	}
}
